<application>
  <component name="CodeInsightSettings">
    <option name="SHOW_FULL_SIGNATURES_IN_PARAMETER_INFO" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="ERROR_STRIPE_MARK_MIN_HEIGHT" value="1" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
    <option name="FONT_FAMILY" value="FiraCode Nerd Font Mono" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
    <option name="LINE_SPACING" value="0.9" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="Hack Nerd Font Mono" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="ECMAScript 6" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_INSPECTION_WIDGET" value="false" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>